<!DOCTYPE html>

<html>
<head>
    <title>Keith Sanders - @ViewBag.Title</title>
    <link rel="icon" type="image/svg" href="~/img/acoustic-guitar.png" />
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <meta name="description" content="keith sanders" />
    <meta name="keywords" content="keith, sanders, music" />

    <link rel="stylesheet" href="~/lib/bootstrap/dist/css/bootstrap.min.css" />
    <link rel="stylesheet" href="~/lib/font-awesome/css/font-awesome.min.css" />
    <link href="https://fonts.googleapis.com/css?family=IM+Fell+English+SC" rel="stylesheet">    @*font-family: 'IM Fell English SC', serif;*@
    <link href="https://fonts.googleapis.com/css?family=Shadows+Into+Light" rel="stylesheet">    @*font-family: 'Shadows Into Light', cursive;*@
    <link href="https://fonts.googleapis.com/css?family=Special+Elite" rel="stylesheet">         @*font-family: 'Special Elite', cursive;*@
    <link href="https://fonts.googleapis.com/css?family=Kalam" rel="stylesheet">                 @*font-family: 'Kalam', cursive;*@
    <link href="https://fonts.googleapis.com/css?family=Cinzel" rel="stylesheet">                @*font-family: 'Cinzel', serif;*@
    <link href="https://fonts.googleapis.com/css?family=Permanent+Marker" rel="stylesheet">      @*font-family: 'Permanent Marker', cursive;*@
    <link href="https://fonts.googleapis.com/css?family=Macondo" rel="stylesheet">               @*font-family: 'Macondo', cursive;*@
    <link href="https://fonts.googleapis.com/css?family=Cormorant+Garamond" rel="stylesheet">    @*font-family: 'Cormorant Garamond', serif;*@

    <link rel="stylesheet" href="~/css/site.css" type="text/css" />    @*REMOVE ?v={random number/string} BEFORE PRODUCTION!!!!!!!*@
    <link rel="stylesheet" href="~/css/index.css" type="text/css" />

 

    <script type="text/javascript" src="~/lib/jquery/dist/jquery.min.js"></script>

    <script type="text/javascript" src="~/lib/bootstrap/dist/js/bootstrap.min.js"></script>


</head>

<body>

    <div id="not-footer-container">
        <div id="lyric-and-controls-container"></div>
        <div id="top-banner">
            <div id="name-title-container">
                <h1>Keith Sanders</h1>
                <h2>Singer | Songwriter</h2>
            </div>
            <div id="navigation-container">
                <a id="index" href="#">Music</a>     @*@Url.Action("Index")*@
                <a id="news" href="#">News</a>              @*href="@Url.Action("News")"*@
                <a id="about" href="#">About</a>        @*href="@Url.Action("About")"*@
            </div>
        </div>
        <div id="page-size"></div>
        <div id="view-and-keith-picture-container">
            <div id="keith-layout-picture"></div>
            <div id="view-container">
                @RenderBody()
            </div>
        </div>
    </div>
    <footer>
        <div id="icon-container">
            <a href="https://www.youtube.com/user/soursweettooth/videos" target="_blank"><img src="~/img/YouTubeIcon.png" alt="YouTube" title="YouTube"/></a>
            <a href="https://www.facebook.com/soursweettooth/" target="_blank"><img src="~/img/FacebookIcon.png" alt="Facebook" title="Facebook"/></a>
            <a href="https://soundcloud.com/sour-sweet-tooth" target="_blank"><img src="~/img/SoundCloudIcon.png" alt="Soundcloud" title="Soundcloud"/></a>
        </div>
        <div id="footer-name-email-container">
            <p>Site by Bill Sanders &copy; 2016</p>
            <a href="mailto:william.sanders.dev@gmail.com">william.sanders.dev@gmail.com</a>
        </div>
    </footer>

    <script type="text/javascript" src="~/js/site.js"></script>
</body>
</html>
